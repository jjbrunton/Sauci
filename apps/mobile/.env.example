# Supabase configuration (required)
# Get these from your Supabase project dashboard: Settings > API
EXPO_PUBLIC_SUPABASE_URL=
EXPO_PUBLIC_SUPABASE_ANON_KEY=

# Sentry error tracking (optional - get DSN from https://sentry.io)
EXPO_PUBLIC_SENTRY_DSN=

# RevenueCat (iOS subscriptions only - Android not supported)
# Get your API key from https://app.revenuecat.com
EXPO_PUBLIC_REVENUECAT_IOS_API_KEY=
# Entitlement ID from RevenueCat dashboard (defaults to "pro" if not set)
EXPO_PUBLIC_REVENUECAT_ENTITLEMENT_ID=Sauci Pro
